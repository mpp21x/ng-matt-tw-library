{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "mattTw",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "matt-tw",
      "kebab-case"
    ],
    "typedef": [
      false,
      "call-signature"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  }
}
